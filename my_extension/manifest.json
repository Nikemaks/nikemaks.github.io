{
  "manifest_version": 2,
  "name": "My First Extension",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}